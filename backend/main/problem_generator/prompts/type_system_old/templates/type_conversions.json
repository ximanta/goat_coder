{
  "cross_language_mappings": {
    "numeric": {
      "java_to_python": {
        "int": "int",
        "Integer": "Optional[int]",
        "double": "float",
        "Double": "Optional[float]"
      },
      "python_to_java": {
        "int": "int",
        "Optional[int]": "Integer",
        "float": "double",
        "Optional[float]": "Double"
      }
    },
    "arrays": {
      "java_to_python": {
        "int[]": "List[int]",
        "String[]": "List[str]"
      },
      "python_to_java": {
        "List[int]": "int[]",
        "List[str]": "String[]"
      }
    }
  }
} 